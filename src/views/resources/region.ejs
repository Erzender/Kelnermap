<div
  style="margin-left: auto; margin-right: auto; width: 80%; min-width: 800px;"
>
<a style="text-decoration: none;" href="<%- "./" + info.region.key -%>"><h1 style="color: orange;">📜<%= info.region.n %></h1></a>
<div style="display: flex; flex-direction: row; align-items: center;">
    <div
      style="width: 20px; height: 20px; border: 3px solid white; margin-right: 10px; background-color: <%- info.domination.color -%>;"
    ></div>
    Domination : <b style="margin-left: 10px;"><%= info.domination.name %></b>
  </div>
  <h2 style="color: cadetblue;">🏠<%= info.edifices.length + " Edifices" %> :</h2>
  <% info.edifices.forEach(function(edifice){ %>
    <div style="font-weight: bold; display: flex; flex-direction: row; align-items: center;">
        <img class="miniProfile" src="<%- edifice.player -%>" />
        <a href="<%- edifice.link -%>"><%= edifice.name %></a>
    </div>
  <% }); %>
</div>
